{
  "printWidth": 100,
  "tabWidth": 2,
  "useTabs": false,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "importOrder": [
    "^react$",
    "<THIRD_PARTY_MODULES>",
    "^@mtfx",
    "style",
    "type",
    "^(api|appconfig|assets|components|configcat.setup|configcat-common|constants|features|globalStore|helpers|hooks|layout|mocks|modules|routes|services|tracking|ui)/?(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "importOrderGroupNamespaceSpecifiers": true,
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy"
  ]
}